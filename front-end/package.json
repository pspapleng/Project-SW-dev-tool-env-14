{
  "name": "moodment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.yarn run 4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.25.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "sass": "^1.49.7",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run cypress:run",
    "eject": "react-scripts eject",
    "jest:unit:run": "jest -i ./src/tests/unit.test.js",
    "jest:unit:cov": "jest --coverage ./src/tests/unit.test.js ./src/app.test.js",
    "cypress:start:app": "cross-env BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "cypress:start:wait": "start-test cypress:start:app http://localhost:3000",
    "cypress:run": "npm run cypress:start:wait -- \"cypress open\"",
    "cypress:cov": "npx nyc report --reporter=text",
    "cypress:component": "npx cypress run -s ./cypress/integration/test.spec.js",
    "cypress:component:run": "cypress run --spec 'cypress/integration/test.spec.js'",
    "cypress:component:cov": "npm run cypress:component:run & npx nyc report --reporter=text",
    "cypress:e2e": "npx cypress run -s ./cypress/integration/e2e.spec.js",
    "cypress:e2e:run": "npm run cypress:start:wait cypress:e2e",
    "cypress:e2e:cov": "npm run cypress:e2e:run & npx nyc report --reporter=text",
    "nyc:report": "nyc report --reporter=cobertura",
    "cypress:e2e:cov:cobertura": "npm-run-all cypress:e2e:run nyc:report",
    "cypress:integration": "npx cypress run -s ./cypress/integration/integration.spec.js",
    "cypress:integration:run": "npm run cypress:start:wait cypress:integration",
    "cypress:integration:cov": "npm run cypress:integration:run & npx nyc report --reporter=text"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.1",
    "cypress-istanbul": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "start-server-and-test": "^1.14.0"
  }
}
